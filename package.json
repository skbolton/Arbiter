{
  "name": "arbiter",
  "version": "2.0.0",
  "description": "A Salesforce ORM powered by jsforce",
  "main": "index.js",
  "tonicExample": "const arbiter = require('arbiter')",
  "engines": {
    "node": ">= 6.0"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/skbolton/Arbiter"
  },
  "scripts": {
    "test": "standard && jest",
    "fix": "standard --fix",
    "test:int": "jest -c=jest-integration-config.json",
    "test:all": "jest -c=jest-all-config.json",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "testRegex": "/__test__/.*.js",
    "clearMocks": true
  },
  "keywords": [
    "ORM",
    "Salesforce",
    "JSForce",
    "Query"
  ],
  "author": "Stephen Bolton",
  "license": "ISC",
  "standard": {
    "env": "jest"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "nock": "^9.2.3",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "dot-prop": "^4.1.1",
    "jsforce": "^1.8.0",
    "ramda": "^0.25.0",
    "superstruct": "^0.5.1",
    "type-detect": "^4.0.0"
  }
}
